{
  "manifest_version": 2,
  "name": "YouTube Music Policy",
  "version": "1.0",

  "description": "A web extention to automate checking large amount of music files via YouTube music policy web-page.",

  "icons": {
    "64": "icons/youtube-64.png"
  },
  
  "content_scripts": [
  	  {
  	  	  "matches": ["*://*.youtube.com/music_policies*"],
  	  	  "js": ["upload-list.js", "thirdparty/jquery-3.4.1.min.js",
  	  	  	  "thirdparty/id3-minimized.js"]
  	  }
  ]
}
